{
   "compilerOptions": {
      "target": "ESNext",
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "allowJs": true,
      "outDir": "./out",
      "noImplicitAny": true,
      "baseUrl": "src",
      "esModuleInterop": true
   },
   "include": [
      "./src",
      "src"
   ],
   "exclude": [
      "./out",
      "./node_modules"
   ]
}